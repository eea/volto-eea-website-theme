@-ms-viewport {
  width: device-width;
}

.clearfix:before,
.clearfix:after,
.row:before,
.row:after {
  display: table;
  content: ' ';
}

.clearfix:after,
.row:after {
  clear: both;
}

.row {
  width: 100%;
  padding: 1rem 0;
}

.col-sm-1,
.col-md-1,
.col-lg-1,
.col-sm-2,
.col-md-2,
.col-lg-2,
