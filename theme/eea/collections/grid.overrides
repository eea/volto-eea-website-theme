/*******************************
         Theme Overrides
*******************************/

.col-example {
  padding: 10px;
  margin-bottom: 12px;
  background: @primaryColor;
  color: @white;
}

.ui.grid > .header.row {
  padding-bottom: 0;
}

.ui.form .ui.grid > .row {
  padding: 0;
}

